{
  "name": "payload-smart-cache",
  "version": "1.0.1",
  "description": "Payload Plugin for Cached Data",
  "keywords": [
    "payload",
    "cached",
    "cache",
    "react",
    "next.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/davincicoding-org/payload-plugins.git",
    "directory": "packages/smart-cache"
  },
  "license": "MIT",
  "author": "Michael Zeltner",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./rsc": {
      "types": "./dist/exports/rsc.d.ts",
      "import": "./dist/exports/rsc.js",
      "default": "./dist/exports/rsc.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "pnpm typecheck",
    "build": "vite build",
    "clean": "rm -rf dist && rm -rf node_modules",
    "dev": "vite build --watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "graph-data-structure": "^4.5.0",
    "lodash-es": "^4.17.21",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@payloadcms/ui": "catalog:payload-stack",
    "@repo/configs": "workspace:*",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.5.4",
    "@types/react": "catalog:payload-stack",
    "@types/react-dom": "catalog:payload-stack",
    "next": "catalog:payload-stack",
    "payload": "catalog:payload-stack",
    "react": "catalog:payload-stack",
    "react-dom": "catalog:payload-stack",
    "rollup-preserve-directives": "catalog:vite",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "catalog:payload-stack",
    "vite": "catalog:vite",
    "vite-plugin-dts": "catalog:vite",
    "vite-tsconfig-paths": "catalog:vite",
    "vitest": "^3.1.2"
  },
  "peerDependencies": {
    "@payloadcms/next": ">=3.72.0",
    "@payloadcms/ui": ">=3.72.0",
    "next": ">=15.2.3",
    "payload": ">=3.72.0"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "publishConfig": {
    "access": "public"
  }
}
