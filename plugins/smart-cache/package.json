{
  "name": "payload-smart-cache",
  "version": "1.2.4",
  "description": "Payload Plugin for Cached Data",
  "keywords": [
    "payload",
    "cached",
    "cache",
    "next.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/davincicoding-org/payload-plugins.git",
    "directory": "plugins/smart-cache"
  },
  "license": "MIT",
  "author": "DAVINCI CODING GmbH",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "pnpm check:types",
    "build": "plugin-kit build",
    "check:types": "tsc --noEmit",
    "dev": "cd dev && pnpm dev",
    "dev:build": "plugin-kit build --watch",
    "generate:types": "plugin-kit generate-types",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "test": "vitest run",
    "test:e2e": "playwright test --config dev/playwright.config.ts",
    "test:int": "vitest run --config dev/vitest.config.mts",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@davincicoding/payload-plugin-kit": "workspace:*",
    "graph-data-structure": "^4.5.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.5.4",
    "next": "catalog:payload-stack",
    "payload": "catalog:payload-stack",
    "typescript": "catalog:payload-stack",
    "vite": "catalog:vite",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "next": ">=15.5.9",
    "payload": ">=3.72.0"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "publishConfig": {
    "access": "public"
  }
}
